        <record model="ir.actions.act_window" id="ks_dashboard_mtn.board_form_tree_action_window">
            <field name="name">لوحة معلومات المدير</field>
            <field name="res_model">ks_dashboard_mtn.board</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{'form_view_ref':'ks_dashboard_mtn.board_form',
                'tree_view_ref':'ks_dashboard_mtn.board_tree'}
            </field>
        </record>

        <record model="ir.actions.act_window" id="ks_dashboard_mtn.template_tree_action_window">
            <field name="name">قالب لوحة المعلومات</field>
            <field name="res_model">ks_dashboard_mtn.board_template</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{'default_ks_template_type':'ks_custom'}
            </field>
        </record>

        <record model="ir.actions.act_window" id="ks_dashboard_mtn.layout_tree_action_window">
            <field name="name">تخطيط لوحة المعلومات</field>
            <field name="res_model">ks_dashboard_mtn.child_board</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[('board_type', '!=', 'default')]</field>
            <field name="context">{'group_by':'ks_dashboard_menu_name'}
            </field>
        </record>

